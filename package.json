{
  "name": "odme",
  "version": "1.1.2",
  "description": "Not a real ODM but will ease dealing with any nosql storage",
  "main": "build/main",
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register",
    "test_cov": "mocha --compilers coffee:coffee-script/register --require blanket --reporter mocha-lcov-reporter"
  },
  "author": "Arash Kay <a.karimzadeh@gmail.com> (http://tectual.com.au/)",
  "license": "ISC",
  "keywords": [
    "odm",
    "nosql"
  ],
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^3.5.0",
    "coffee-script": "^1.9.0",
    "coveralls": "^2.11.2",
    "groc": "^0.8.0",
    "gulp": "^3.8.10",
    "gulp-coffee": "^2.2.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "1.2.0",
    "puffer": "^1.0.1"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "boom": "^4.2.0",
    "joi": "^10.2.2",
    "json-mask": "^0.3.8",
    "lodash": "^4.17.4",
    "shortid": "^2.1.3"
  },
  "config": {
    "blanket": {
      "pattern": "build",
      "data-cover-never": "node_modules",
      "loader": "./node-loaders/coffee-script"
    }
  }
}
